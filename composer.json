  "description": "Fork from https://github.com/TYPO3-extensions/purge",
  "homepage": "https://github.com/zbychu555/purge",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS", "Purge"],
  "support": {
    "issues": "https://github.com/zbychu555/purge"
  },
  "require": {
    "typo3/cms-core": "^6.2.14 || ^7.6.0"
  },
  "autoload": {
